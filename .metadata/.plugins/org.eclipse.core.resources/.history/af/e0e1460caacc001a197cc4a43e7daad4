package List;

public class ListMain {

	public static void main(String[] args) {
		Node n1 =new Node(100);
		Node n2 =new Node(200);
		Node n3 =new Node(300);
		
		n1.next = n2;
		n2.next = n3;
		n3.next = null;
		
		Node head  = n1;
		print(head);
	}
	static void print(Node node) {
		Node temp = node;
		while(temp !=null) {
			System.out.printf("[%d]=>",temp.value);
			temp = temp.next;
		}
	}

}
