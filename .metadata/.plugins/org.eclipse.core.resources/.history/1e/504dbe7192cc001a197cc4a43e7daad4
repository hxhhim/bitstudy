package Queue;

import java.util.Arrays;

public class Queue {
	int[] arr;
	int front = 0;
	@Override
	public String toString() {
		return "Queue [arr=" + Arrays.toString(arr) + ", front=" + front + ", rear=" + rear + "]";
	}
	int rear = 0;
	public Queue(){
		this(5);
	}
	public Queue(int no) {
		arr = new int[no];
	}
	boolean isFull() {
		return rear ==arr.length;
	}
	public void put(int val) {
		if(isFull()) {
			System.out.println("큐가 찼습니다.");
			return;
		}
		arr[rear++] = val;
	}
	public int get() {
		return arr[front++];
	}
}
